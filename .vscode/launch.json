    "version": "0.2.0",
    "configurations": [        
        {
            "name": "gql_cache_lens + simulated environment",
            "request": "launch",
            "program": "lib/main.dart",
            "type": "dart",
            "toolArgs": [
                "--dart-define", "use_simulated_environment=true"
            ],
        },
        {
            "name": "example app (debug)",
            "cwd": "example",
            "request": "launch",
            "program": "lib/main.dart",
            "type": "dart",
            "args": [
                "--debug",
            ],
            "toolArgs": [
                "--dart-define-from-file", ".env"
            ]
        },
    ]
}