    "version": "0.2.0",
    "configurations": [
        {
            "name": "gql_cache_lens + simulated environment",
            "cwd": "gql_cache_lens",
            "request": "launch",
            "type": "dart",
            "args": [
                "--dart-define=use_simulated_environment=true"
            ]
        },
        {
            "name": "gql_cache_lens (profile mode)",
            "cwd": "gql_cache_lens",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "gql_cache_lens (release mode)",
            "cwd": "gql_cache_lens",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}